@{
    var localizedTitles = new string[,]
    {
        { "zh-HK", "頁面未找到" },
        { "zh", "页面未找到" },
        { "ar", "الصفحة غير موجودة" },
        { "he", "הדף לא נמצא" },
        { "fa", "صفحه یافت نشد" },
        { "ur", "صفحہ نہیں ملا" },
        { "ja", "ページが見つかりません" },
        { "th", "ไม่พบหน้านี้" },
        { "ko", "페이지를 찾을 수 없습니다" },
        { "el", "Η σελίδα δεν βρέθηκε" },
        { "hi", "पृष्ठ नहीं मिला" },
        { "en", "Page Not Found" }
    };

    ViewData["Title"] = LocalizationHelper.GetLocalizedValue(localizedTitles);
}

<localize>
    <case culture="zh-HK">
        <h1>頁面未找到</h1>
        <p>您查找的頁面不存在。</p>
    </case>

    <case culture="zh">
        <h1>页面未找到</h1>
        <p>您查找的页面不存在。</p>
    </case>

    <case culture="ar">
        <h1>الصفحة غير موجودة</h1>
        <p>الصفحة التي تبحث عنها غير موجودة.</p>
    </case>

    <case culture="he">
        <h1>הדף לא נמצא</h1>
        <p>הדף שאתה מחפש אינו קיים.</p>
    </case>

    <case culture="fa">
        <h1>صفحه یافت نشد</h1>
        <p>صفحه‌ای که به دنبال آن هستید وجود ندارد.</p>
    </case>

    <case culture="ur">
        <h1>صفحہ نہیں ملا</h1>
        <p>جس صفحہ کی آپ تلاش کر رہے ہیں وہ موجود نہیں ہے۔</p>
    </case>

    <case culture="ja">
        <h1>ページが見つかりません</h1>
        <p>お探しのページは存在しません。</p>
    </case>

    <case culture="th">
        <h1>ไม่พบหน้านี้</h1>
        <p>หน้าที่คุณกำลังค้นหาไม่มีอยู่</p>
    </case>

    <case culture="ko">
        <h1>페이지를 찾을 수 없습니다</h1>
        <p>찾고 계신 페이지가 존재하지 않습니다.</p>
    </case>

    <case culture="el">
        <h1>Η σελίδα δεν βρέθηκε</h1>
        <p>Η σελίδα που αναζητάτε δεν υπάρχει.</p>
    </case>

    <case culture="hi">
        <h1>पृष्ठ नहीं मिला</h1>
        <p>आप जिस पृष्ठ की तलाश कर रहे हैं वह मौजूद नहीं है।</p>
    </case>

    <case culture="en">
        <h1>Page Not Found</h1>
        <p>The page you are looking for does not exist.</p>
    </case>
</localize>
